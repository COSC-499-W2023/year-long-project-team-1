{
  "name": "privacypal",
  "version": "0.1.0-dev",
  "private": true,
  "scripts": {
    "start:dev": "PORT=${PORT:-8081} next dev",
    "build": "next build",
    "clean": "rimraf .next",
    "lint": "next lint",
    "test": "jest --coverage",
    "test:usnapshot": "jest --coverage -u",
    "license:add": "license-check-and-add add -f license-config.json",
    "license:check": "license-check-and-add check -f license-config.json",
    "prisma:generate": "prisma generate --schema=db/schema.prisma",
    "cypress:open": "APP_PORT=${APP_PORT:-8081} cypress open",
    "cypress:run": "cypress run --browser firefox --e2e --headless"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.462.0",
    "@aws-sdk/lib-storage": "^3.462.0",
    "@patternfly/react-core": "^5.1.1",
    "@patternfly/react-icons": "^5.1.1",
    "@patternfly/react-styles": "^5.1.1",
    "@patternfly/react-table": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "iron-session": "^6.3.1",
    "next": "14.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.33.0",
    "swr": "^2.2.4"
  },
  "devDependencies": {
    "@prisma/client": "^5.6.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/bcrypt": "^5.0.1",
    "@types/bcryptjs": "^2.4.5",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.9.5",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "autoprefixer": "^10.4.16",
    "cypress": "^13.6.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "license-check-and-add": "^4.0.5",
    "postcss": "^8.4.32",
    "prettier": "3.1.0",
    "prisma": "^5.6.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  }
